{
  "name": "@meow-double/eslint",
  "description": "Eslint config for your code",
  "license": "MIT",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [
    "eslint"
  ],
  "author": {
    "name": "Meow-DoubleðŸ¦ˆ",
    "url": "https://github.com/Meow-Double"
  },
  "repository": {
    "url": "https://github.com/Meow-Double/meowdouble-configs/tree/main/configs/eslint"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    ".eslint.cjs"
  ],
  "scripts": {
    "lint:ts": "npx eslint --config .eslintrc.cjs \"src/**/*.{ts,tsx}\"",
    "lint:js": "npx eslint --config .eslintrc.cjs \"src/**/*.{js,jsx}\"",
    "lint:ts:fix": "npx eslint --config .eslintrc.cjs \"src/**/*.{ts,tsx}\" --fix",
    "lint:js:fix": "npx eslint --config .eslintrc.cjs \"src/**/*.{js,jsx}\" --fix"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^8.57.1",
    "eslint-plugin-custom": "file:rules/custom/eslint-plugin-custom",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/utils": "^8.32.0"
  }
}
